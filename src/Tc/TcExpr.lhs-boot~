checking expressions

> tcExpr :: Expr Name -> TcM ([Pred Name], Ty Name)

> tcAlts :: Alts Name -> TcM ([Pred Name], Ty Name)

>> tcAlts' :: Ty Name -> Alts Name -> TcM ([Pred Name], Ty Name)

> tcAlt :: Alt Name -> TcM ([Pred Name], Ty Name)
